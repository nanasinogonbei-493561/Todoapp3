# データベース設定
MYSQL_URL=jdbc:mysql://localhost:3306/todo_prod?useSSL=false&serverTimezone=UTC&allowPublicKeyRetrieval=true
MYSQL_USERNAME=your_username
MYSQL_PASSWORD=your_secure_password

# データベース接続プール設定
DB_POOL_SIZE=10
DB_MIN_IDLE=5
DB_CONNECTION_TIMEOUT=30000
DB_IDLE_TIMEOUT=600000
DB_MAX_LIFETIME=1800000

# サーバー設定
SERVER_PORT=8080
SESSION_TIMEOUT=30m

# アプリケーション設定
APP_NAME=Todo Application
APP_VERSION=1.0.0
APP_ENVIRONMENT=production

# セキュリティ設定
COOKIE_SECURE=true
COOKIE_HTTP_ONLY=true
COOKIE_SAME_SITE=strict

# ログ設定（オプション）
LOG_LEVEL=WARN
LOG_FILE_PATH=logs/application.json

# Java設定
JAVA_HOME=/Users/sugawara/Library/Java/JavaVirtualMachines/ms-17.0.15/Contents/Home 